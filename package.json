  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.15.0",
    "browserslist": "^4.20.2",
    "graphql": "^14.7.0",
    "if-env": "^1.0.4",
    "react-apollo": "^3.1.5"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
